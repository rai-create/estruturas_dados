public class ArrayStack implements Stackable{

    // array para armazenar os elementos da pilha
private Object[] data;

    //indice no topo da pilha
private int pointerTop;


// construtor que nao tem parametros e chama outro construtor usando tamanho 10
public ArrayStack(){
this(10);

}

//construtor para definir o tamanho da pilha
public ArrayStack(int lenght){
    data = new Object[lenght];
    pointerTop = -1;

}

// verifica se a pilha esta vazia
boolean isEmpty(){
    return pointerTop == -1;
}

// verifica se a pilha esta cheia
boolean isFull(){
    return pointerTop == data.lenght -1;
}

